<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jf.javafx.plugins.menu.impl.ManagementController">
   <children>
      <SplitPane fx:id="pane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" dividerPositions="0.4" >
         <items>
            <VBox>
               <children>
                  <HBox prefWidth="200.0" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <TextField fx:id="txtNewMenu" promptText="%menuLabel.text" HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#onAddNewMenu_click" text="%addNewBtn.text" />
                        <Button mnemonicParsing="false" onAction="#onDeleteMenu_click" text="%deleteBtn.text" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
                  <TreeView fx:id="treeView" prefHeight="200.0" prefWidth="300.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
