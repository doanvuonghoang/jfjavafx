<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="jfjavafx.plugins.genreport.GenReportFormController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="%EntGDTCode.text" />
            <TextArea fx:id="txtEntGDTCode" minHeight="100.0" prefHeight="100.0" prefWidth="200.0" />
            <TextField fx:id="txtOutDir" promptText="Output dir" />
            <HBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <VBox prefHeight="200.0" spacing="10.0" HBox.hgrow="NEVER">
                     <children>
                        <ComboBox fx:id="cbConnection" prefWidth="150.0" />
                        <Button mnemonicParsing="false" onAction="#onGen_click" text="%Gen.text" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
